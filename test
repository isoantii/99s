local function checklisting(uid, gems, item, version, shiny, amount, username, playerid, class)
    gems = tonumber(gems)
    local type = {}
    pcall(function()
        type = Library.Directory.Pets[item]
end)

    if class =="Charm" and gems <= 50000 then
        game:GetService("ReplicatedStorage").Network.Booths_RequestPurchase:InvokeServer(playerid, uid)
    if type.exclusiveLevel and gems <= 50000 and class == "Pet" then
        game:GetService("ReplicatedStorage").Network.Booths_RequestPurchase:InvokeServer(playerid, uid)
    elseif type.supeiorLevel and gems <=10 and class == "Pet" then
        game:GetService("ReplicatedStorage").Network.Booths_RequestPurchase:InvokeServer(playerid, uid)
    elseif type.mythicalLevel and gems <=750 and class == "Item" then
        game:GetService("ReplicatedStorage").Network.Booths_RequestPurchase:InvokeServer(playerid, uid)
    elseif type.legendaryLevel and gems <=550 and class == "Item" then
        game:GetService("ReplicatedStorage").Network.Booths_RequestPurchase:InvokeServer(playerid, uid)
    elseif type.mythicalLevel and gems <=1000 and class == "Charm" then
        game:GetService("ReplicatedStorage").Network.Booths_RequestPurchase:InvokeServer(playerid, uid)
    elseif type.legendaryLevel and gems <=1000 and class == "Charm" then
        game:GetService("ReplicatedStorage").Network.Booths_RequestPurchase:InvokeServer(playerid, uid)
    elseif string.find(item, "Exclusive") and gems <= 25000 then
        game:GetService("ReplicatedStorage").Network.Booths_RequestPurchase:InvokeServer(playerid, uid)
    elseif type.huge and gems <= 2500000 then
        game:GetService("ReplicatedStorage").Network.Booths_RequestPurchase:InvokeServer(playerid, uid)
    elseif type.titanic and gems <= 100000000 then
        game:GetService("ReplicatedStorage").Network.Booths_RequestPurchase:InvokeServer(playerid, uid)
    elseif type.legendaryLevel and gems <=100 and class == "Enchant" then
        game:GetService("ReplicatedStorage").Network.Booths_RequestPurchase:InvokeServer(playerid, uid)
    elseif type.mythicalLevel and gems <=1000 and class == "Enchant" then
        game:GetService("ReplicatedStorage").Network.Booths_RequestPurchase:InvokeServer(playerid, uid)
    elseif type.exoticLevel and gems <=10000 and class == "Enchant" then
        game:GetService("ReplicatedStorage").Network.Booths_RequestPurchase:InvokeServer(playerid, uid)
    elseif type.superiorLevel and gems <=100000 and class == "Enchant" then
        game:GetService("ReplicatedStorage").Network.Booths_RequestPurchase:InvokeServer(playerid, uid)
    elseif type.exclusiveLevel and gems <=1000 and class == "Enchant" then
        game:GetService("ReplicatedStorage").Network.Booths_RequestPurchase:InvokeServer(playerid, uid)
    end
end

Booths_Broadcast.OnClientEvent:Connect(function(username, message)
    if message == nil then return end
    local playerID = message['PlayerID']
    if type(message) == "table" then
        local listing = message["Listings"]
        for key, value in pairs(listing) do
            if type(value) == "table" then
                local uid = key
                local gems = value["DiamondCost"]
                local itemdata = value["ItemData"]

                if itemdata then
                    local data = itemdata["data"]

                    if data then
                        local item = data["id"]
                        local version = data["pt"]
                        local class = itemdata["class"]
                        local shiny = data["sh"]
                        local amount = data["_am"]
                        checklisting(uid, gems, item, version, shiny, amount, username , playerID, class)
                    end
                end
            end
        end
    end
end)
